<div class="printer">

    {{#with printer}}
    <p>Title:{{title}}</p>
    <p>ip:{{ip}}</p>
    <p>model:{{model}}</p>
    <p>isMultifunctional:{{isMultifunctional}}</p>
    <p>Area:{{area}}</p>
    <p>Location:{{location}}</p>
    {{#if history}}
        <p>
            <a class="btn btn-primary" data-bs-toggle="collapse" href="#collapse1" role="button" aria-expanded="false" aria-controls="collapseExample">
               History
            </a>
            <a class="btn btn-primary" data-bs-toggle="collapse" href="#collapse2" role="button" aria-expanded="false" aria-controls="collapseExample">
                Add supply
            </a>
            <a class="btn btn-warning">
                Modify
            </a>

        </p>
        <div class="collapse" id="collapse1">
            {{#each history}}
                <ul class="list-group">
                    <li class="list-group-item" aria-current="true">{{supply}} - installed at {{installedAt}}</li>
                </ul>
            {{/each}}
        </div>


    {{/if}}

    {{/with}}

    <div class="collapse" id="collapse2">
        <ul class="list-group">
            {{#each supplies}}
                <li class="list-group-item" aria-current="true">{{supply}} - <a href="" class="btn btn-success">add</a></li>
            {{/each}}
        </ul>
    </div>

</div>